local ReplicatedStorage = game:GetService("ReplicatedStorage")
local hasilDump = "=== HASIL DUMP JARINGAN ETZO ===\n"

local success, err = pcall(function()
    local Packages = ReplicatedStorage:WaitForChild("Packages", 5)
    local _Index = Packages:WaitForChild("_Index", 5)
    local sleitnick = _Index:WaitForChild("sleitnick_net@0.2.0", 5)
    local net = sleitnick:WaitForChild("net", 5)

    local count = 0
    for _, v in ipairs(net:GetChildren()) do
        hasilDump = hasilDump .. "Tipe: " .. v.ClassName .. " | Nama: " .. v.Name .. "\n"
        count = count + 1
    end
    hasilDump = hasilDump .. "\nTotal Remote: " .. count
end)

if success then
    -- Perintah rahasia untuk menyalin teks otomatis di Delta
    if setclipboard then
        setclipboard(hasilDump)
        print("✅ SUKSES! Teks sudah otomatis disalin (Copy). Silakan Paste!")
    else
        print("❌ Delta Lite versi ini tidak mendukung setclipboard.")
    end
else
    print("Gagal: " .. tostring(err))
end
